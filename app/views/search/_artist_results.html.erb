<br />
<table class="table">
	<thead>
	</thead>
	<tbody>
	<% if @search.respond_to?(:artistmatches) and @search.artistmatches.size > 0 %>
		<%= render :partial => 'artist_result', :collection => @search.artistmatches %>
	<% else %>
		<tr>
			<td>
				No artists found based on your search query
			</td>
		</tr>
	<% end %>
	</tbody>
</table>
