<h1>Search results:</h1>
<br />
<table class="table table-striped table-bordered table-condensed">
	  <thead>
  <tr>
    <th>Artist</th>
    <th>URL</th>
  </tr>
  </thead>
  <tbody>
<% @results['results']['artistmatches']['artist'].each do |artist| %>

	<tr>
		<td><%= artist['name'] %></td>
		<td><%= link_to(artist['url'], artist['url']) %></td>
	</tr>
		
<% end %>
 </tbody>
</table>